{
  "name": "@domp/create",
  "version": "0.1.3",
  "description": "DOM, HTML",
  "main": "cjs/index.js",
  "jsnext:main": "es/index.mjs",
  "module": "es/index.mjs",
  "scripts": {
    "clean": "rimraf cjs dist es",
    "lint": "eslint --ext .js --ext .mjs src",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir cjs",
    "build:es": "BABEL_ENV=es babel src --keep-file-extension --out-dir es",
    "build:node": "BABEL_ENV=node babel src --out-dir node",
    "build:umd": "mkdir -p dist && BABEL_ENV=umd rollup -c -o dist/domp-create.js",
    "build:umd:min": "mkdir -p dist && BABEL_ENV=umd rollup -c | uglifyjs -c -m > dist/domp-create.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "preversion": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lohfu/domp-create.git"
  },
  "keywords": [
    "DOM",
    "HTML"
  ],
  "author": "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lohfu/domp-create/issues"
  },
  "homepage": "https://github.com/lohfu/domp-create#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "babel-plugin-add-module-exports": "^0.2.1",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^4.0.0-beta.2"
  }
}
