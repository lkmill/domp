{
  "name": "@domp/fp",
  "version": "0.0.1",
  "description": "Convert regular dom-packages to auto-curried, iteratee-first and data-last methods",
  "main": "./index",
  "scripts": {
    "clean": "rimraf dist lib",
    "lint": "eslint --ext .js --ext .mjs src __tests__",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:umd:min",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib",
    "build:esm": "BABEL_ENV=esm babel src --keep-file-extension --out-dir lib",
    "build:umd": "mkdir -p dist && BABEL_ENV=umd rollup -c -o dist/domp-fp.js",
    "build:umd:min": "mkdir -p dist && BABEL_ENV=umd rollup -c | uglifyjs -c -m > dist/domp-fp.min.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "test": "BABEL_ENV=cjs jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dom-packages/fp.git"
  },
  "author": "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dom-packages/fp/issues"
  },
  "homepage": "https://github.com/dom-packages/fp#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.2"
  }
}
