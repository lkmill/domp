{
  "name": "@domp/find",
  "version": "0.2.2",
  "description": "DOM, HTML",
  "main": "lib/index",
  "scripts": {
    "clean": "rimraf dist lib",
    "lint": "eslint --ext .js --ext .mjs src",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:umd:min",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib",
    "build:esm": "BABEL_ENV=esm babel src --keep-file-extension --out-dir lib",
    "build:umd": "mkdir -p dist && BABEL_ENV=umd rollup -c -o dist/find.js",
    "build:umd:min": "mkdir -p dist && BABEL_ENV=umd rollup -c | uglifyjs -c -m > dist/find.min.js",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dom-packages/find.git"
  },
  "keywords": [
    "DOM",
    "HTML"
  ],
  "author": "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dom-packages/find/issues"
  },
  "homepage": "https://github.com/dom-packages/find#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "dependencies": {
    "@domp/create": "^0.2.0"
  }
}
